{
  "name": "angular-redux-rxjs-clean-architecture",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start:local": "cross-env APP_ENV=local ng serve -o",
    "start:dev": "cross-env APP_ENV=development ng serve -o --configuration development",
    "build:dev": "ng build --configuration development && npm run compress-replace-images",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration production && npm run remove-uncompressed-files && npm run compress-replace-images",
    "start:build:prod": "npx http-server dist/angular-redux-rxjs-clean-architecture -g",
    "test:local": "ng test",
    "test:dev": "ng test --browsers=ChromeHeadless",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint --fix",
    "prettier:check": "npx prettier --check .",
    "prettier:apply": "npx prettier --write .",
    "remove-uncompressed-files": "node scripts/post-build/remove-uncompressed-files.mjs",
    "compress-replace-images": "node scripts/post-build/compress-replace-images.mjs",
    "version:commit": "sh scripts/versioning/updateProjectVersion.sh",
    "release-notes:commit": "sh scripts/versioning/finalizeReleaseNotes.sh",
    "tag-version-with-notes:commit": "sh scripts/versioning/tagVersionWithNotes.sh",
    "docker:build:dev": "sh scripts/docker/build-image.sh development",
    "docker:build:prod": "sh scripts/docker/build-image.sh production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.3",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.3",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@auth0/auth0-angular": "^2.2.1",
    "process": "^0.11.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-devkit/build-angular": "^16.2.2",
    "@angular-eslint/builder": "16.1.2",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/schematics": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/cli": "~16.2.2",
    "@angular/compiler-cli": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/dotenv-webpack": "^7.0.4",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fs": "^0.0.1-security",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-overwrite": "^7.0.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  }
}
